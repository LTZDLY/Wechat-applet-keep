/* pages/video/video.wxss */
.header {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  height: 300rpx;
  line-height: 300rpx;
  background-color: rgba(0, 0, 255, 0.3);
  margin-bottom: 50rpx;
  position: fixed;
}
.empty {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  height: 300rpx;
  line-height: 300rpx;
  margin-bottom: 50rpx;
}

.info{
  height: 100rpx;
  width: 100%;
  margin-bottom: 30rpx;
}

.other{
  height: 80rpx;
  width: 100%;
  margin-bottom: 20rpx;
}

.tab-h{
  height: 600rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 80rpx;
  font-size: 16px;
  white-space: nowrap;
  top: 0;
  left: 0;
  z-index: 99;
}

.mvlist{
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 1500rpx;
  margin-bottom: 50rpx;
  background-color: rgba(255, 0, 0, 0.3);
}
